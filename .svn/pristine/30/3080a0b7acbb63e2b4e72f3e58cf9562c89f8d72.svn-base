import Vue from 'vue'
import Router from 'vue-router'
import App from './App.vue'
import Home from './views/Home.vue'

Vue.use(Router)

export default new Router({
  mode: 'history',
  base: process.env.BASE_URL,
  routes: [
    {
      path: '/',
      component: Home,
      redirect: '/index'
    },
    {
      path: '/',
      component: Home,
      children: [
        {
          path: '/index',
          component: () => import('@/views/indexManage')
        }
      ]
    },
  ]
})
